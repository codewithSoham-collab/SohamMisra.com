src/main/java/com/sohammisra/mediaplayer/MainActivity.java
package com.sohammisra.mediaplayer;

import android.app.Activity;
import android.content.Intent;
import android.os.Bundle;
import android.view.View;
import android.widget.Button;
import android.widget.Toast;

public class MainActivity extends Activity {
    
    @Override
    protected void onCreate(Bundle savedInstanceState) {
        super.onCreate(savedInstanceState);
        setContentView(R.layout.activity_main);
        
        Button btnMusic = findViewById(R.id.btnMusic);
        Button btnVideos = findViewById(R.id.btnVideos);
        Button btnPlaylists = findViewById(R.id.btnPlaylists);
        Button btnEqualizer = findViewById(R.id.btnEqualizer);
        
        btnMusic.setOnClickListener(new View.OnClickListener() {
            @Override
            public void onClick(View v) {
                Intent intent = new Intent(MainActivity.this, PlayerActivity.class);
                intent.putExtra("type", "music");
                startActivity(intent);
            }
        });
        
        btnVideos.setOnClickListener(new View.OnClickListener() {
            @Override
            public void onClick(View v) {
                Intent intent = new Intent(MainActivity.this, PlayerActivity.class);
                intent.putExtra("type", "video");
                startActivity(intent);
            }
        });
        
        btnPlaylists.setOnClickListener(new View.OnClickListener() {
            @Override
            public void onClick(View v) {
                Intent intent = new Intent(MainActivity.this, PlaylistActivity.class);
                startActivity(intent);
            }
        });
        
        btnEqualizer.setOnClickListener(new View.OnClickListener() {
            @Override
            public void onClick(View v) {
                Intent intent = new Intent(MainActivity.this, EqualizerActivity.class);
                startActivity(intent);
            }
        });
        
        // Show welcome message
        Toast.makeText(this, "Welcome to Ultimate Media Player by Soham Misra!", Toast.LENGTH_LONG).show();
    }
}

